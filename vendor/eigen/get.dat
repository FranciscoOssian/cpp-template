@echo off

rem Enter the vendor/eigen directory
cd vendor\eigen

rem Download the Eigen file using curl
curl -o eigen.tar.gz -L https://gitlab.com/libeigen/eigen/-/archive/3.4.0/eigen-3.4.0.tar.gz

rem Extract the Eigen files without creating an additional directory
7z x eigen.tar.gz -o. -y

rem Remove the downloaded archive file
del eigen.tar.gz

rem Go back to the original directory
cd ..\..
